<?xml version="1.0"?>

<PropertyList>

  <path>g115.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fuselage</object-name>
    <object-name>interieur</object-name>
    <object-name>ailes</object-name>
    <object-name>cacheA</object-name>
    <object-name>axeA</object-name>
    <object-name>roueA</object-name>
    <object-name>cacheG</object-name>
    <object-name>axeGB</object-name>
    <object-name>axeGH</object-name>
    <object-name>roueG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeDB</object-name>
    <object-name>axeDH</object-name>
    <object-name>roueD</object-name>
    <object-name>bol</object-name>
    <object-name>helice</object-name>
    <object-name>tourverriere</object-name>
    <object-name>derive</object-name>
    <object-name>direction</object-name>
    <object-name>aileronG</object-name>
    <object-name>aileronD</object-name>
    <object-name>voletG</object-name>
    <object-name>voletD</object-name>
    <object-name>echappe</object-name>
    <object-name>antenne1</object-name>
    <object-name>pitot</object-name>
    <object-name>air</object-name>
    <object-name>tourlampeG</object-name>
    <object-name>tourlampeD</object-name>
    <object-name>tourlampe2</object-name>
    <object-name>ajout1</object-name>

    <!-- Objets transparents -->
    <object-name>vitre</object-name>
    <object-name>verriere</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>lampeG</object-name>
    <object-name>lampeD</object-name>
    <object-name>lampe2</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
    <object-name>verriere</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>lampeG</object-name>
    <object-name>lampeD</object-name>
    <object-name>lampe2</object-name>
  </animation>

  <!-- L'hélice --> 
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>600.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.674 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.177 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>controls/flight/rudder</property>
    <factor>20</factor>
    <axis>
      <x1-m>  3.387 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m>  0.054 </z1-m>
      <x2-m>  3.609 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  1.263 </z2-m>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>controls/flight/elevator</property>
    <factor>11</factor>
    <center>
      <x-m>  3.134 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.005 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor>15.0</factor>
    <axis>
      <x1-m> -0.916 </x1-m>
      <y1-m> -4.817 </y1-m>
      <z1-m> -0.266 </z1-m>
      <x2-m> -0.781 </x2-m>
      <y2-m> -3.327 </y2-m>
      <z2-m> -0.383 </z2-m>
    </axis>
  </animation> 

  <animation>
    <type>rotate</type>
    <object-name>aileronD</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <factor>15.0</factor>
    <axis>
      <x1-m> -0.781 </x1-m>
      <y1-m>  3.327 </y1-m>
      <z1-m> -0.383 </z1-m>
      <x2-m> -0.916 </x2-m>
      <y2-m>  4.817 </y2-m>
      <z2-m> -0.266 </z2-m>
    </axis>
  </animation>

  <!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>voletG</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>50.0</factor>
    <axis>
      <x1-m> -0.781 </x1-m>
      <y1-m> -3.327 </y1-m>
      <z1-m> -0.399 </z1-m>
      <x2-m> -0.554 </x2-m>
      <y2-m> -0.818 </y2-m>
      <z2-m> -0.601 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>voletD</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>50.0</factor>
    <axis>
      <x1-m> -0.554 </x1-m>
      <y1-m>  0.818 </y1-m>
      <z1-m> -0.601 </z1-m>
      <x2-m> -0.781 </x2-m>
      <y2-m>  3.327 </y2-m>
      <z2-m> -0.399 </z2-m>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <type>translate</type>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <object-name>axeA</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> -0.1 </dep>
      </entry>
      <entry>
        <ind> 1.000 </ind>
        <dep> 0.150 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> 0.056539 </x>
      <y>-0.000412 </y>
      <z> 0.203463 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeGB</object-name>
    <object-name>cacheG</object-name>
    <object-name>roueG</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.0 </ind>
        <dep> 10.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -1.008 </x-m>
      <y-m> -1.004 </y-m>
      <z-m> -0.871 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeDB</object-name>
    <object-name>cacheD</object-name>
    <object-name>roueD</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.0 </ind>
        <dep> 10.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -1.008 </x-m>
      <y-m>  1.004 </y-m>
      <z-m> -0.871 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <property>controls/flight/rudder</property>
    <factor> 10.0 </factor>
    <center>
      <x-m> -2.789 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.234 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z> -1.000 </z>
    </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.789 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.234 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.030 </x-m>
      <y-m> -1.281 </y-m>
      <z-m> -1.238 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <object-name>essieuD</object-name>
    <object-name>disqueD</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.030 </x-m>
      <y-m>  1.281 </y-m>
      <z-m> -1.238 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Canopy -->
  <animation>
    <type>translate</type>
    <object-name>verriere</object-name>
    <object-name>tourverriere</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <factor>1</factor>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Interior -->

 <model>
  <path>Aircraft/Grob-G115/Models/GrobInterior.xml</path>
  <offsets>
   <x-m>-1.8</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.2</z-m>
  </offsets>
 </model>

  <!-- White Strobes -->

 <model>
  <path>Aircraft/Grob-G115/Models/strobe-white.xml</path>
  <offsets>
   <x-m>-1.60</x-m>
   <y-m>-5.00</y-m>
   <z-m>-0.25</z-m>
  </offsets>
 </model>

 <model>
  <path>Aircraft/Grob-G115/Models/strobe-white.xml</path>
  <offsets>
   <x-m>-1.60</x-m>
   <y-m>5.00</y-m>
   <z-m>-0.25</z-m>
  </offsets>
 </model>

  <!-- Red Strobe -->

 <model>
  <path>Aircraft/Grob-G115/Models/strobe-red.xml</path>
  <offsets>
   <x-m>-0.44</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.80</z-m>
  </offsets>
 </model>

</PropertyList>

