<?xml version="1.0"?>
<PropertyList>

 <path>strobe-white.osg</path>
  <offsets>
    <x-m> 0.0 </x-m>
    <y-m> 0.0 </y-m>
    <z-m> 0.0 </z-m>
 </offsets>

  <animation>
    <type>select</type>
    <object-name>strobe.osg</object-name>
    <condition>
      <greater-than>
        <property>systems/electrical/outputs/white-strobe</property>
        <value>20</value>
      </greater-than>
    </condition>
  </animation>


</PropertyList>
