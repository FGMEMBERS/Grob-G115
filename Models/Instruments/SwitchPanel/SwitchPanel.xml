   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>nasal</command>
    <script>controls.startEngine()</script>
   </binding>
   <mod-up>
    <binding>
     <command>nasal</command>
     <script>props.setAll("/controls/engines/engine","starter", 0)</script>
    </binding>
   </mod-up>                     
  </action>
 </animation>

