<?xml version="1.0"?>

<PropertyList>

 <path>FP-CHT.ac</path>

 <animation> 
  <type>rotate</type>
  <object-name>PtrL</object-name>
  <object-name>PtrTipL</object-name>
  <property>engines/engine/oil-temperature-degf</property>
  <interpolation>
   <entry><ind>104</ind><dep> 15</dep></entry>
   <entry><ind>140</ind><dep> 45</dep></entry>
   <entry><ind>212</ind><dep> 75</dep></entry>
   <entry><ind>266</ind><dep>105</dep></entry>
  </interpolation>
  <axis> 
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>-0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
 <animation> 
  <type>rotate</type>
  <object-name>PtrR</object-name>
  <object-name>PtrTipR</object-name>
  <property>engines/engine/oil-pressure-psi</property>
  <interpolation>
   <entry><ind>  0</ind><dep> 15</dep></entry>
   <entry><ind> 30</ind><dep> 50</dep></entry>
   <entry><ind> 90</ind><dep> 90</dep></entry>
   <entry><ind>120</ind><dep>105</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
