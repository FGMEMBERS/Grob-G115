<?xml version="1.0"?>
<PropertyList>

 <path>navlight-green.ac</path>

 <animation>
  <type>select</type>
  <object-name>Plane</object-name>
  <condition>
   <greater-than>
    <property>controls/lighting/nav-lights-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Plane</object-name>
  <emission>
   <red>1.0</red>
   <green>1.0</green>
   <blue>1.0</blue>
   <factor-prop>controls/lighting/nav-lights-norm</factor-prop>
  </emission>
 </animation>

</PropertyList>
